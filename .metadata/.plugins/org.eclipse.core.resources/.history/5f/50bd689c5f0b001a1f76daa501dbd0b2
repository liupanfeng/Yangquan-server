import java.util.List;
import org.apache.ibatis.annotations.Param;

public interface AdOpenScreenMapper {
    int countByExample(AdOpenScreenExample example);

    int deleteByExample(AdOpenScreenExample example);

    int deleteByPrimaryKey(Integer id);

    int insert(AdOpenScreen record);

    int insertSelective(AdOpenScreen record);

    List<AdOpenScreen> selectByExample(AdOpenScreenExample example);

    AdOpenScreen selectByPrimaryKey(Integer id);

    int updateByExampleSelective(@Param("record") AdOpenScreen record, @Param("example") AdOpenScreenExample example);

    int updateByExample(@Param("record") AdOpenScreen record, @Param("example") AdOpenScreenExample example);

    int updateByPrimaryKeySelective(AdOpenScreen record);

    int updateByPrimaryKey(AdOpenScreen record);
}